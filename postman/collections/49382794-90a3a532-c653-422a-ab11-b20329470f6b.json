				{
					"name": "Get grades",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "cfd99708-e188-491a-ae5e-786a87a5c25d",
								"exec": [
									"// داده‌های JSON از پاسخ API",
									"const data = pm.response.json();",
									"",
									"// بررسی صحت داده‌ها",
									"if (data && Array.isArray(data)) {",
									"    // فیلتر فقط پایه‌های 1 تا 9 (ابتدایی و متوسطه اول)",
									"    const filteredData = data.filter(item => ",
									"        item.slug === \"primary\" || item.slug === \"midlevel\"",
									"    );",
									"",
									"    // تابع برای انتخاب تصادفی از آرایه",
									"    const getRandomItem = (array) => array[Math.floor(Math.random() * array.length)];",
									"",
									"    // انتخاب تصادفی یک grade از بین ابتدایی و متوسطه اول",
									"    const randomGrade = getRandomItem(filteredData);",
									"    const gradeId = randomGrade.id;",
									"",
									"    // انتخاب تصادفی یک level از grade انتخاب‌شده",
									"    const randomLevel = getRandomItem(randomGrade.levels);",
									"    const levelId = randomLevel.id;",
									"",
									"    // بررسی وجود field_of_studies و انتخاب تصادفی یا null",
									"    const fieldOfStudy = randomLevel.field_of_studies && randomLevel.field_of_studies.length > 0",
									"        ? getRandomItem(randomLevel.field_of_studies).id",
									"        : null;",
									"",
									"    // ذخیره در متغیرهای Postman",
									"    pm.environment.set(\"gradeId\", gradeId);",
									"    pm.environment.set(\"levelId\", levelId);",
									"    pm.environment.set(\"fieldOfStudyId\", fieldOfStudy);",
									"",
									"    // چاپ برای بررسی (اختیاری)",
									"    console.log(\"Grade ID:\", gradeId);",
									"    console.log(\"Level ID:\", levelId);",
									"    console.log(\"Field of Study ID:\", fieldOfStudy);",
									"    console.log(\"انتخاب از:\", randomGrade.name, \"-\", randomLevel.name);",
									"} else {",
									"    console.log(\"داده‌های ورودی نامعتبر است\");",
									"}",
									"",
									"// اصلاح تستِ city_id: خواندن از Environment و اطمینان از تعریف بودن و خالی نبودن",
									"const envCityId = pm.environment.get(\"city_id\");",
									"pm.test(\"مقدار city_id ذخیره شده در Environment\", function () {",
									"    pm.expect(envCityId, \"city_id should exist in active environment\").to.not.be.undefined;",
									"    pm.expect(String(envCityId).trim(), \"city_id should not be empty\").to.not.equal(\"\");",
									"",
									"    // خواندن شناسه‌ها از Environment برای جلوگیری از خطای scope",
									"    const loggedGradeId = pm.environment.get(\"gradeId\");",
									"    const loggedLevelId = pm.environment.get(\"levelId\");",
									"    console.log(\"Grade ID:\", loggedGradeId);",
									"    console.log(\"Level ID:\", loggedLevelId);",
									"    console.log(\"city_id:\", envCityId);",
									"});",
									"",
									"// لاگ شفاف در صورتی که city_id موجود نباشد",
									"if (envCityId === undefined || String(envCityId).trim() === \"\") {",
									"    console.log(\"هشدار: مقدار city_id در Environment تعریف نشده یا خالی است.\");",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
